{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "rememberitwholesale",
    "projects": {
        "rememberitwholesale": {
            "projectType": "application",
            "schematics": {},
            "root": "",
            "sourceRoot": "src",
            "prefix": "riw",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/rememberitwholesale",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": ["zone.js"],
                        "tsConfig": "tsconfig.app.json",
                        "assets": ["src/favicon.ico", "src/assets"],
                        "styles": [
                            "src/assets/css/styles.css",
                            "src/assets/css/primeng-override.css",
                            "src/assets/themes/lara-dark-teal_theme_v16.css",
                            "node_modules/primeicons/primeicons.css",
                            "node_modules/primeng/resources/primeng.min.css"
                        ],
                        "allowedCommonJsDependencies": [
                            "is-mobile",
                            "date-fns"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "outputHashing": "all",
                            "fileReplacements": [
                              {
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.prod.ts"
                              }
                            ]
                        },
                        "development": {
                            "verbose": true,
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": false,
                            "namedChunks": true,
                            "fileReplacements": [
                              {
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.dev.ts"
                              }
                            ]
                        },
                        "live": {
                          "verbose": true,
                          "buildOptimizer": false,
                          "optimization": false,
                          "vendorChunk": true,
                          "extractLicenses": false,
                          "sourceMap": false,
                          "namedChunks": true,
                          "fileReplacements": [
                            {
                              "replace": "src/environments/environment.ts",
                              "with": "src/environments/environment.live.ts"
                            }
                          ]
                      }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "rememberitwholesale:build:production"
                        },
                        "development": {
                            "buildTarget": "rememberitwholesale:build:development"
                        },
                        "live": {
                          "buildTarget": "rememberitwholesale:build:live"
                        }
                    },
                    "defaultConfiguration": "development"
                }
            }
        }
    }
}
