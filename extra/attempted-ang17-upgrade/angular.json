{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "rememberitwholesale",
    "projects": {
        "rememberitwholesale": {
            "projectType": "application",
            "schematics": {},
            "root": "",
            "sourceRoot": "src",
            "prefix": "riw",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/rememberitwholesale",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": ["zone.js"],
                        "tsConfig": "tsconfig.app.json",
                        "assets": ["src/favicon.ico", "src/assets"],
                        "styles": [
                            "src/styles.css",
                            "src/primeng-override.css",
                            "node_modules/primeicons/primeicons.css",
                            "node_modules/primeng/resources/themes/lara-dark-teal/theme.css",
                            "node_modules/primeng/resources/primeng.min.css"
                        ],
                        "allowedCommonJsDependencies": [
                            "is-mobile"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "outputHashing": "all"
                        },
                        "development": {
                            "verbose": true,
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": false,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                          "buildTarget": "rememberitwholesale:build:production"
                        },
                        "development": {
                          "buildTarget": "rememberitwholesale:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                }
            }
        }
    }
}
