{
  "name": "rememberitwholesale",
  "version": "1.0",
  "scripts": {
    "dev": "ng serve --watch --configuration development --no-live-reload",
    "prod": "ng serve --watch --configuration production --host 0.0.0.0",
    "backend-dev": "NODE_ENV=development node --watch --watch-preserve-output backend-node/maindata.js",
    "backend": "NODE_ENV=production node --watch --watch-preserve-output backend-node/maindata.js",
    "build": "ng build --configuration production",
    "postbuild": "minify src/public.html > dist/public.html || echo '> If minify failed, install it'"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.8",
    "@angular/cdk": "^18.2.8",
    "@angular/common": "^18.2.8",
    "@angular/compiler": "^18.2.8",
    "@angular/core": "^18.2.8",
    "@angular/forms": "^18.2.8",
    "@angular/platform-browser": "^18.2.8",
    "@angular/platform-browser-dynamic": "^18.2.8",
    "@angular/router": "^18.2.8",
    "compression": "^1.8.1",
    "config": "^4.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^4.21.1",
    "express-fileupload": "^1.5.2",
    "express-rate-limit": "^8.2.1",
    "is-mobile": "^5.0.0",
    "marked": "^15.0.12",
    "node-mailjet": "^6.0.11",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.11",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.5",
    "tslib": "^2.8.1",
    "uuid": "^13.0.0",
    "yazl": "^3.3.1",
    "zone.js": "^0.14.10"
  },
  "devDependencies": {
    "@angular/build": "^18.2.8",
    "@angular/cli": "^18.2.8",
    "@angular/compiler-cli": "^18.2.8",
    "typescript": "^5.5.4"
  }
}
